#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
   int y;
   scanf("%d",&y);
   if(y>999&&y<10000)
   {    
    if(y%4==0)
    { 
       if(y%100==0)
       {
           if(y%400==0)
             printf("LEAP");
           else
             printf("NOT LEAP");   
       }
        else
            printf("LEAP");
    }
    else
        printf("NOT LEAP");
   }
    else
        printf("Invalid Input");
    return 0;
}
